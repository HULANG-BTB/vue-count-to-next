{
  "name": "vue-count-to-next",
  "version": "1.0.1",
  "description": "It's a vue3 component that will count to a target number at a specified duration",
  "author": "AloneH <admin@oibit.cn>",
  "main": "lib/index.umd.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/HULANG-BTB/vue-count-to-next"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:web": "vue-cli-service build",
    "build": "yarn build:umd && yarn build:esm",
    "build:umd": "rollup --config ./build/rollup.config.umd.js",
    "build:esm": "rollup --config ./build/rollup.config.esm.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "3.0.7"
  },
  "peerDependencies": {
    "vue": "^3.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "vetur": {
    "tags": "./vetur/tags.json",
    "attributes": "./vetur/attributes.json"
  }
}
